<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pan.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DSKit">

        <activity android:name=".DSKitActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".toolbar.ToolbarsActivity">
        </activity>

        <activity android:name=".buttons.ButtonsActivity">
        </activity>

        <activity android:name=".labels.LabelsActivity">
        </activity>

        <activity android:name=".seekbar.SliderActivity">
        </activity>

        <activity android:name=".edittext.EditTextActivity">
        </activity>

        <activity android:name=".radiogroup.RadioGroupActivity">
        </activity>

        <activity android:name=".items.SummaryItem.SummaryItemActivity">
        </activity>

        <activity android:name=".items.ImageInfoItem.ImageInfoItemActivity">
        </activity>

    </application>

</manifest>