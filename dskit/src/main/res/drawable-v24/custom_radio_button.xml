<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:ignore="MissingDefaultResource">
    <item
        android:state_checked="true" android:drawable="@drawable/ic_checked">
        <shape
            android:shape="oval">
            <stroke android:color="#888888" android:width="8dp"/>
            <solid android:color="@color/dodger_blue"/>
        </shape>
    </item>
    <item
        android:state_checked="false" android:drawable="@drawable/ic_unchecked"/>
</selector>